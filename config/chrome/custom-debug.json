{
    "include": [
        "../../ts/Custom/*.ts"
    ],
    "compilerOptions":
    {
        "outDir": "../../js/custom",
        "removeComments": false,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "target": "es6",
        "inlineSourceMap": true,
        "allowJs": false,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noImplicitThis": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "./node_modules/*"
    ]
}