{
    "include": [
        "../../ts/Custom/*.ts"
    ],
    "compilerOptions":
    {
        "outDir": "../../js/custom",
        "removeComments": true,
        "strictNullChecks": true,
        "strict": true,
        "noImplicitAny": true,
        "target": "es6",
        "inlineSourceMap": false,
        "allowJs": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noImplicitThis": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true
    }
}