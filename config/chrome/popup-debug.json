{
    "include": [
        "../../ts/Chrome/ChromePopupStarter.ts"
    ],
    "compilerOptions":
    {
        "outFile": "../../js/popup.js",
        "removeComments": false,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "target": "es5",
        "inlineSourceMap": true,
        "allowJs": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noImplicitThis": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "lib": [
            "es6", "dom"
        ]
    }
}