{
    "include": [
        "../../ts/Chrome/ChromePopupStarter.ts"
    ],
    "compilerOptions":
    {
        "outFile": "../../js/popup.js",
        "removeComments": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "target": "es6",
        "inlineSourceMap": false,
        "allowJs": false,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noImplicitThis": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}